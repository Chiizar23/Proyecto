package rpg.entities.enemies.enemyTypeE;

import rpg.entities.enemies.Enemy;

public class Hechicero extends Enemy {

    public Hechicero(String name, int health) {
        super(name, health);
    }

    @Override
    public void attack() {
        System.out.println(name + " lanza un hechizo oscuro.");
    }
}
